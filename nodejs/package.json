{
  "name": "ict-dashboard-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "8.1.2"
  },
  "dependencies": {
    "@atlaskit/tooltip": "^1.2.0",
    "@gov.au/datavizkit": "^1.0.27-development",
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "date-fns": "^1.28.5",
    "freactal": "^1.1.1",
    "lodash": "4.17.16",
    "numeral": "^1.5.6",
    "polished": "^1.4.0",
    "prop-types": "^15.5.10",
    "query-string": "^4.3.4",
    "react": "15.6.1",
    "react-collapse": "^4.0.3",
    "react-dom": "15.6.1",
    "react-fittext": "^1.0.0",
    "react-height": "^3.0.0",
    "react-media": "^1.6.1",
    "react-motion": "^0.5.0",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "recompose": "^0.24.0",
    "redux": "3.7.0",
    "redux-thunk": "2.2.0",
    "sassdash": "^0.8.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@storybook/react": "^3.1.7",
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "^2.0.1",
    "css-loader": "0.28.1",
    "deepmerge": "^1.5.0",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.3",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.6",
    "slack-notify": "^0.1.6",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "yarn run test -- --updateSnapshot",
    "test:ci": "CI=true yarn run test -- --coverage --runInBand --colors",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy-storybook": "npm run build-storybook",
    "deploy-storybook": "surge -p storybook-static -d ict-projects-dashboard-ui.surge.sh",
    "postdeploy-storybook": "yarn run notify-slack:storybook",
    "notify-slack:storybook": "node ./bin/notify-slack.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "coveragePathIgnorePatterns": [
      "(.story)\\.(js)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "globals": {
      "__DEV__": true
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
